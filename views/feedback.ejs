    <title>Document</title>
    <link rel="stylesheet" href="/style.css" />
    <link rel="stylesheet" href="/tailwind.css" />
    <script src="https://cdn.tailwindcss.com"></script>
    <style>
      body {
        margin: 0;
        text-align: center;
        display: flex;
        justify-content: center;
        align-items: center;
        min-height: 100vh;
        font-family: sans-serif;
        background-color: #111722;
        color: white;
      }
      .feedback-wrapper {
        width: 90%;
        max-width: 500px;
        margin: 0 auto;
        background-color: #222c40;
        padding: 25px 18px;
      }
      .container {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        align-items: center;
      }
      .item {
        width: 90px;
        height: 90px;
        display: flex;
        justify-content: center;
        align-items: center;
        user-select: none;
      }
      .radio {
        display: none;
      }
      .radio ~ span {
        font-size: 3rem;
        filter: grayscale(100);
        cursor: pointer;
        transition: 0.3s;
      }
      .radio:checked ~ span {
        filter: grayscale(0);
        font-size: 4rem;
      }
      .feedback {
        width: 100%;
      }
      .feedback textarea,
      .feedback input {
        max-width: 300px;
        width: 100%;
        display: block;
        margin: 0.5em auto;
        padding: 0.5em;
        font-family: inherit;
        border: 2px solid transparent;
        /* border: 1px solid #d1d2d7; */
        border-radius: 3px;
        background-color: #374151;
        color: white;
      }
      .feedback textarea:focus,
      .feedback textarea:active,
      .feedback input:focus,
      .feedback input:active {
        border-color: #1d4ed8;
        box-shadow: 0px 0px 0px 3px rgba(29, 78, 216, 0.2);
        transition: 100ms;
        outline: 0;
      }
      .feedback textarea {
        height: 100px;
      }
      .feedback button {
        margin: 1em auto;
        display: table;
        text-align: center;
      }
      .feedback button:focus,
      .feedback button:active {
        outline: 0;
      }
    </style>
  </head>
